<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mockvoting.domain.report.mapper.ReportTypeMapper">
    <select id="selectAllReportTypes" resultType="com.example.mockvoting.domain.report.dto.ReportTypeResponseDTO">
        SELECT id, code, name, description
        FROM report_type
        ORDER BY sort_order
    </select>

</mapper>